<h1>test</h1>

<%= simple_form_for [@transaction, @review] do |f| %>
  <div class="form-group">
    <%= f.label :rating %>
    <%= f.collection_select :rating, 1..5, :to_s, :to_s, prompt: true, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :content %>
    <%= f.input :content, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :cleanliness_rating %>
    <%= f.collection_select :cleanliness_rating, 1..5, :to_s, :to_s, prompt: true, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :maintenence_rating %>
    <%= f.collection_select :maintenence_rating, 1..5, :to_s, :to_s, prompt: true, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :accuracy_rating %>
    <%= f.collection_select :accuracy_rating, 1..5, :to_s, :to_s, prompt: true, class: "form-control" %>
  </div>
  <div class="actions">
    <%= f.button :submit, class: "btn btn-primary" %>
  </div>
<% end %>
